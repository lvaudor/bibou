@article{ WOS:000237891600014,
Author = {Knispel, Sandra and Sartori, Michel and Brittain, John E.},
Title = {Egg development in the mayflies of a Swiss glacial floodplain},
Journal = {JOURNAL OF THE NORTH AMERICAN BENTHOLOGICAL SOCIETY},
Year = {2006},
Volume = {25},
Number = {2},
Pages = {430-443},
Month = {JUN},
Abstract = {The duration and timing of egg development is crucial to the survival of
   invertebrates in glacier-fed rivers. The relationship between egg
   development and environmental conditions was investigated in 3 mayfly
   species, Baetis alpinus, Rhithrogena nivata, and Ecdyonurus picteti, in
   an alpine glacier-fed alluvial system. Field-collected eggs were reared
   in the laboratory at temperatures representing field conditions (1.5, 3,
   5, and 7 degrees C). Egg-incubation period and degree-days demand (DD)
   were determined, and hatching in the field was predicted from field
   temperature records. Egg development and temperature relationships for
   B. alpinus and R. nivata were documented for the first time. Baetis
   alpinus had synchronous egg development and high hatching success.
   Faster development in warmer habitats enabled it to hatch during
   favorable autumnal conditions. Ecdyonurus picteti had a very long
   development time that decreased slightly at higher temperatures.
   However, the variation in DD was considerable. The observed long and
   delayed hatching would favor successful development in unpredictable
   habitats at the low temperatures experienced in these glacial
   conditions. Rhithrogena nivata, one of the most extreme cold-adapted
   mayfly species, had a long egg incubation period, and success in
   development largely depended on the timing of hatching and discharge
   conditions. This species could exploit extremely unstable and cold
   habitats where other species are limited by low water temperatures.},
Publisher = {NORTH AMER BENTHOLOGICAL SOC},
Address = {1041 NEW HAMSPHIRE STREET, LAWRENCE, KS 66044 USA},
Type = {Article},
Language = {English},
Affiliation = {Knispel, S (Corresponding Author), Musee Cantonal Zool, Palais Rumine, CH-1014 Lausanne, Switzerland.
   Musee Cantonal Zool, CH-1014 Lausanne, Switzerland.
   Univ Oslo, Nat Hist Museum, NO-0318 Oslo, Norway.},
DOI = {10.1899/0887-3593(2006)25{[}430:EDITMO]2.0.CO;2},
ISSN = {0887-3593},
Keywords = {embryonic development; egg rearing; incubation period; degree-days
   demand; hatching; glacial streams; low temperature; Rhone; Swiss Alps;
   Baetis alpinus; Rhithrogena nivata; Ecdyonurus picteti},

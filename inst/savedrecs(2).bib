@article{ WOS:000246956100005,
Author = {Picon, Bernard and Allard, Paul},
Title = {Floods of the Rhone river downstream in 1856, 1993, 1994, 2003 and their
   impacts on society},
Journal = {HOUILLE BLANCHE-REVUE INTERNATIONALE DE L EAU},
Year = {2007},

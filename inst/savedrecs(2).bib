@article{ WOS:000174634800007,
Author = {Durand, N and Fiandrino, A and Fraunié, P and Ouillon, S and Forget, P
   and Naudin, JJ},
Title = {Suspended matter dispersion in the Ebro ROFI:: an integrated approach},

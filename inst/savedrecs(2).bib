@inproceedings{ WOS:000241916501052,
Author = {Bollaert, E. and Andre, S. and Diouf, S. and Sidler, D. and Ferretti, S.},
Editor = {Ferreira, RML and Alves, CTL and Leal, GAB and Cardoso, AH},
Title = {Long-term morphological behavior of Verbois reservoir (Geneva)},
Booktitle = {RIVER FLOW 2006, VOLS 1 AND 2},
Series = {Proceedings and Monographs in Engineering Water and Earth Sciences},
Year = {2006},
Pages = {1577+},
Note = {International Conference on Fluvial Hydraulics, Lisbon, PORTUGAL, SEP
   06-08, 2006},
Abstract = {This paper presents a study of the long-term morphological behaviour of
   the Verbois reservoir in Geneva, Switzerland. The reservoir has been
   constructed on the Rhone River downstream of Geneva in 1942 and has a
   total length of 12 km. Sedimentation problems appeared immediately after
   dam construction, with an averaged yearly deposited volume of fine
   sediments of about 400,000 m(3). To minimize sedimentation, flushing
   events have been performed on a tri-annual basis. These generate huge
   enviromnental impact..To deal with this, a study has been performed of
   the potential morphological behaviour of the reservoir in the absence of
   further flushing events for the next 50 years. Analytical and numerical
   computations have been performed over a total river and reservoir length
   of about 25 km. These indicate that the reservoir will continue its
   sedimentation rather quickly during the first 10-15 years after stoppage
   of flushing, followed by a progressive stabilization of the
   sedimentation, resulting in a dynamic equilibrium on the long term. This
   equilibrium corresponds to a main meandering channel with sediments
   along the inner bends of the former Rhone River.},

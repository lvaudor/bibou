@article{ WOS:000173022600001,
Author = {Laubscher, H},
Title = {Plate interactions at the southern end of the Rhine graben},
Journal = {TECTONOPHYSICS},
Year = {2001},
Volume = {343},
Number = {1-2},
Pages = {1-19},
Month = {DEC 15},
Abstract = {The southern end of the Rhine graben in the Basel area originated in the
   early Tertiary as an extensional structure. it is the site of a transfer
   zone that connected the Rhine graben with the Bresse-Rhone grabens. This
   transfer zone is located in an area into which a late Paleozic trough
   projects, which apparently was reactivated. From the beginning of
   Miocene to recent times, three distinct phases of slight compression
   were superimposed on the southern end of the Rhine graben. The first one
   in the Aquitanian-early Burdigalian is coeval with the development of
   the Helvetic nappes and the Insubric fault zone in the Alps. The second
   one in the Langhian-Tortonian interval occurred when the External
   Massifs and an important part of the Lombardic fold-thrust belt in the
   southern Alps developed, and the third, Pliocene to Recent one, is
   contemporaneous with the inactivation of this belt and its incorporation
   into the foredeep of the Northern Apennines. In the Aquitanian, the
   forebulge of the Alpine orogen is recognizable as a system of flexures
   that developed preferably on the margin of the late Paleozoic trough.
   The middle-late Miocene one gave rise to uplift of the Black
   Forest-Vosges domain and a river system flowing from this high to the
   south. In the Pliocene to Recent one Alpine rivers made their
   appearance, flowing first to the west into the Rhone system and then, as
   the Rhine, to the north. The Pliocene to Recent phase seems to be
   decoupled from the body of the Central and Southern Alps. (C) 2001
   Elsevier Science B.V. All rights reserved.},

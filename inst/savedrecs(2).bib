@article{ WOS:000253503200004,
Author = {Wildi, Walter and Koukal, Brahim and Ischi, Vanessa and Perroud, Antoine},
Title = {Sediment quality of the Verbois reservoir, comparison with sediments of
   Swiss rivers and Lake Geneva},
Journal = {ARCHIVES DES SCIENCES},
Year = {2006},

@article{ WOS:000246942100002,
Author = {Fan, Ying and Miguez-Macho, Gonzalo and Weaver, Christopher P. and
   Walko, Robert and Robock, Alan},
Title = {Incorporating water table dynamics in climate modeling: 1. Water table
   observations and equilibrium water table simulations},
Journal = {JOURNAL OF GEOPHYSICAL RESEARCH-ATMOSPHERES},
Year = {2007},
Volume = {112},
Number = {D10},
Month = {MAY 31},
Abstract = {Soil moisture is a key participant in land-atmosphere interactions and
   an important determinant of terrestrial climate. In regions where the
   water table is shallow, soil moisture is coupled to the water table.
   This paper is the first of a two-part study to quantify this coupling
   and explore its implications in the context of climate modeling. We
   examine the observed water table depth in the lower 48 states of the
   United States in search of salient spatial and temporal features that
   are relevant to climate dynamics. As a means to interpolate and
   synthesize the scattered observations, we use a simple two-dimensional
   groundwater flow model to construct an equilibrium water table as a
   result of long-term climatic and geologic forcing. Model simulations
   suggest that the water table depth exhibits spatial organization at
   watershed, regional, and continental scales, which may have implications
   for the spatial organization of soil moisture at similar scales. The
   observations suggest that water table depth varies at diurnal, event,
   seasonal, and interannual scales, which may have implications for soil
   moisture memory at these scales.},

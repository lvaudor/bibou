@article{ WOS:000224474900015,
Author = {Fiandino, M and Martin, C},
Title = {Fluxes of suspended matter in the coastal rivers towards the Pond of
   Berre (Bouches-du-Rhone, France)},
Journal = {HOUILLE BLANCHE-REVUE INTERNATIONALE DE L EAU},
Year = {2004},
Number = {4},
Pages = {97-101},
Abstract = {Tond of Berre is a large briny water pond in communication with the
   Mediterranean sea. It is a discharge system of three main rivers
   presenting a marked Mediterranean flow; the Arc, the Touloubre and the
   Cadiere. For several decades, the Pond of Berre undergoes a serious
   dysfunction due to the high level of the anthropogenic activities of the
   catchment area and, since 1966, to contributions of fresh water and
   sediments coming from the Durance by EDF channel. The present study
   proposes the quantification of fluxes of suspended matter, coming from
   the Arc, the Touloubre and the Cadiere, from September 1998 to December
   2000 and the extension in the time of the obtained results (period
   1961-2000). Over the period 1961-2000, the average annual specific
   computed values of suspended matter are 35.5 l km(2) for the Arc, 17.4
   t/km(2) for the Touloubre and 47.7 t/km(2) for the Cadiere, Nearly 30\%
   as of the suspended matter carried towards the Pond of Berre originate
   actually on its catchment area Zsum723.},

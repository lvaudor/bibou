@article{ WOS:000221450000004,
Author = {Bollache, L and Devin, S and Wattier, R and Chovet, M and Beisel, JN and
   Moreteau, JC and Rigaud, T},
Title = {Rapid range extension of the Ponto-Caspian amphipod <i>Dikerogammarus
   villosus</i> in France:: potential consequences},
Journal = {ARCHIV FUR HYDROBIOLOGIE},
Year = {2004},
Volume = {160},

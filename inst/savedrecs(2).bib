@inproceedings{ WOS:000238048200007,
Author = {Provansal, Mireille and Vella, Claude and Sabatier, Fracois},
Book-Group-Author = {Institut Oceanographique},
Title = {Sedimentaty inputs and mobility of deltaic coasts during the Holocene},
Booktitle = {DYNAMIQUE SEDIMENTAIRE EN ZONE COTIERE},
Series = {Oceanis-Serie de Documents Oceanographiques},
Year = {2006},
Volume = {29},
Number = {1-2},
Pages = {209-238},
Note = {Seminar on Sedimentary Dynamics in Coastal Zone, Paris, FRANCE, JAN
   15-22, 2002},
Organization = {Inst Oceanograph},
Abstract = {Fluvial solid discharges have determined the historic mobility of
   deltaic coastlines. This relation is clearly demonstrated by the
   Mediterranean deltas during the Little Ice Age (16-19th c.) and the late
   Holocene. The deltaic progradation phases are associated with the more
   abundant and coarser sediment inputs, due to anthropic and climatic
   forcing in the fluvial basin. Its interruption by fluvial avulsion and
   mouth shift are analysed in the Rhone delta. The accommodation volume in
   front of the mouth and the geomorphology of the channel modify the
   relationships between the fluvial input and the mobility of the deltaic
   coastlines. Since the 19th c., river management has induced the final
   advances of the deltas.},

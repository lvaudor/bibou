@article{ WOS:000176281600008,
Author = {Ottlé, C and Richard, D and LeHégart, S and Guérin, C and Noilhan, J and
   LeMoigne, P and Habets, F},
Title = {Hydro-meteorological modelisation of the Rhone riverbasin:: contribution
   of remote sensing data},
Journal = {HOUILLE BLANCHE-REVUE INTERNATIONALE DE L EAU},

   IEEE},
Title = {Alpine snow cover and glaciers in relation to altitude from advanced
   satellite monitoring},
Booktitle = {IGARSS 2001: SCANNING THE PRESENT AND RESOLVING THE FUTURE, VOLS 1-7,
   PROCEEDINGS},
Series = {IEEE International Symposium on Geoscience and Remote Sensing (IGARSS)},

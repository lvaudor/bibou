@article{ WOS:000223308300003,
Author = {Scotti, M and Jimenez, C},
Title = {Contribution of physical models to safety of river works: examples on
   the Rhone river},
Journal = {HOUILLE BLANCHE-REVUE INTERNATIONALE DE L EAU},
Year = {2004},
Number = {3},
Pages = {25-30},
Abstract = {Taking into account the large number of hydraulic works exposed to high
   constraints; day to day maintenance of a large developed river like the
   Rhone needs permanent supervisions. It is especially the case for
   hydraulic works subjected to large downstream energy dissipation,
   because their ageing is faster and because their safety is more
   important than those of other hydraulic works.
   Examples presented in the paper, dealing with the downstream apron
   stability of the SEYSSEL dam and the BEAUCAIRE weir, show that physical
   modeling is an essential ``tool{''} to help hydraulic works maintenance
   teams to solve complex technical problems, at the diagnosis state and at
   the repairing works design stage as well.
   Fiability of physical modelling results allow hydraulic works
   maintenance managers to optimize survey and repairing works investments
   without any risk for environment and riverine population.},

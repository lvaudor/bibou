@article{ WOS:000188387100001,
Author = {Darnaude, AM and Salen-Picard, C and Polunin, NVC and Harmelin-Vivien,
   ML},
Title = {Trophodynamic linkage between river runoff and coastal fishery yield
   elucidated by stable isotope data in the Gulf of Lions (NW
   Mediterranean)},
Journal = {OECOLOGIA},
Year = {2004},
Volume = {138},
Number = {3},
Pages = {325-332},
Month = {FEB},
Abstract = {The link between climate-driven river runoff and sole fishery yields
   observed in the Gulf of Lions (NW Mediterranean) was analysed using
   carbon- and nitrogen stable isotopes along the flatfish food webs. Off
   the Rhone River, the main terrestrial (river POM) and marine (seawater
   POM) sources of carbon differed in delta(13)C (-26.11parts per thousand
   and -22.36parts per thousand, respectively). Surface sediment and
   suspended POM in plume water exhibited low delta(13)C (-24.38parts per
   thousand and -24.70parts per thousand, respectively) that differed more
   from the seawater POM than from river POM, demonstrating the dominance
   of terrestrial material in those carbon pools. Benthic invertebrates
   showed a wide range in delta(15)N (mean 4.30parts per thousand to
   9.77parts per thousand) and delta(13)C (mean -23.81parts per thousand to
   -18.47parts per thousand), suggesting different trophic levels, diets
   and organic sources. Among the macroinvertebrates, the surface (mean
   delta(13)C -23.71parts per thousand) and subsurface (mean delta(13)C
   -23.81parts per thousand) deposit-feeding polychaetes were particularly
   C-13 depleted, indicating that their carbon was mainly derived from
   terrestrial material. In flatfish, delta(15)N (mean 9.42 to 10.93parts
   per thousand) and delta(13)C (mean -19.95parts per thousand to
   -17.69parts per thousand) varied among species, indicating differences
   in food source and terrestrial POM use. A significant negative
   correlation was observed between the percentage by weight of polychaetes
   in the diet and the delta(13)C of flatfish white muscle. Solea solea
   (the main polychaete feeder) had the lowest mean delta(13)C, Arnoglossus
   laterna and Buglossidium luteum (crustacean, mollusc and polychaete
   feeders) had intermediate values, and Solea impar (mollusc feeder) and
   Citharus linguatula (crustacean and fish feeder) exhibited the highest
   delta(13)C. Two different benthic food webs were thus identified off the
   Rhone River, one based on marine planktonic carbon and the other on the
   terrestrial POM carried by the river. Deposit-feeding polychaetes were
   responsible for the main transfer of terrestrial POM to upper trophic
   levels, linking sole population dynamics to river runoff fluctuations.},

@article{ WOS:000173296300005,
Author = {Durand, P},
Title = {Mauvoisin dam - Service and bottom outlet raising project Environmental
   impacts},
Journal = {HOUILLE BLANCHE-REVUE INTERNATIONALE DE L EAU},

@article{ WOS:000253503200017,
Author = {von Arx, Bertrand and Bal, Bernard and Beuchat, Sebastien},
Title = {Cross-boundary conservation and management (France-Switzerland) of
   natural areas in the Leman basin. The foremost stake regarding the Rhone
   River and the INTERREG III project},

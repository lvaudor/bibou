@article{ WOS:000176613400006,
Author = {Dethier, M and Castella, E},
Title = {A ten years survey of longitudinal zonation and temporal changes of
   macrobenthic communities in the Rhone River, downstream from lake Geneva
   (Switzerland)},
Journal = {ANNALES DE LIMNOLOGIE-INTERNATIONAL JOURNAL OF LIMNOLOGY},
Year = {2002},
Volume = {38},
Number = {2},
Pages = {151-162},
Abstract = {The macrobenthic fauna of the Rhone River below Lake Geneva was studied
   between 1983 and 1993 to describe patterns of longitudinal and temporal
   community changes. Sampling was based upon the use of artificial
   substrates in 11 stations. The confluence with the Arve (an alpine
   tributary) is the first source of faunal discontinuity, both in terms of
   species and feeding group composition. The Verbois hydropower plant is
   the second longitudinal disruption. From a temporal point of view, the
   construction of the Seujet hydropower plant was a major source of faunal
   fluctuation. After this event, and until the end of the study,
   lenitophilous taxa increased to the detriment of rheophilous taxa.},

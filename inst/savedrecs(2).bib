@article{ WOS:000176093800005,
Author = {Liébault, F and Clément, P and Piégay, H and Rogers, CF and Kondolf, GM
   and Landon, N},
Title = {Contemporary channel changes in the Eygues basin, southern French
   Prealps:: the relationship of subbasin variability to watershed
   characteristics},

@article{ WOS:000187789000007,
Author = {Nemirovskaya, IA},
Title = {Hydrocarbons in the system of the White Sea},
Journal = {OCEANOLOGY},
Year = {2003},
Volume = {43},
Number = {S},
Pages = {S85-S98},
Abstract = {Data on the content and composition of aliphatic and polycyclic aromatic
   hydrocarbons (HC) in marine aerosols, in both dissolved and suspended
   forms in the surface and deep waters, as well as in the bottom sediments
   of the White Sea, are presented. Natural terrigenous compounds prevail
   in the composition of HC. The HC of anthropogenic (oil) composition are
   of a confined character and related to the ports and to the most
   navigable areas. Biogenic autochthonous or mixed (oil-allochthonous)
   compounds were found in coastal areas, because the oil HC promote the
   intensification of biogenic processes. In the external (biological) zone
   of the Severnaya Dvina River-sea marginal filter, along with aliphatic
   hydrocarbons, the formation of polycyclic aromatic hydrocarbons takes
   place.},

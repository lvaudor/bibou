@article{ WOS:000237511800018,
Author = {Lafont, M and Vivier, A and Nogueira, S and Namour, P and Breil, P},
Title = {Surface and hyporheic oligochaete assemblages in a French suburban
   stream},
Journal = {HYDROBIOLOGIA},

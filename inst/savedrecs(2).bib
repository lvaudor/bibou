@article{ WOS:000220457200006,
Author = {Caballero, Y and Chevallier, P and Gallaire, R and Pillco, R},
Title = {Flow modelling in a high mountain valley equipped with hydropower
   plants: Rio zongo valley, Cordillera Real, Bolivia},

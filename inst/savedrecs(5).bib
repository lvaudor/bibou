Title = {Sediment dynamics in the Gulf of Lion (NW Mediterranean Sea) during two
   autumn-winter periods with contrasting meteorological conditions},
Journal = {PROGRESS IN OCEANOGRAPHY},
Year = {2023},
Volume = {210},

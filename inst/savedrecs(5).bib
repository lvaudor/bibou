@article{ WOS:000843278800001,
Author = {Coussin, V. and Penaud, A. and Combourieu-Nebout, N. and Peyron, O. and
   Schmidt, S. and Zaragosi, S. and de Vernal, A. and Babonneau, N.},
Title = {Distribution of modern dinocysts and pollen in the western Mediterranean
   Sea (Algerian margin and Gulf of Lion)},

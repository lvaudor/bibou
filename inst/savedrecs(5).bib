@article{ WOS:000616338400012,
Author = {Flaminio, Silvia},
Title = {Modern and Nonmodern Waters: Sociotechnical Controversies, Successful
   Anti-Dam Movements and Water Ontologies},
Journal = {WATER ALTERNATIVES-AN INTERDISCIPLINARY JOURNAL ON WATER POLITICS AND
   DEVELOPMENT},
Year = {2021},

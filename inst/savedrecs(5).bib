@article{ WOS:000679610700001,
Author = {Bernoulli, Daniel and Capuzzo, Nicola},
Title = {The voyage of the `Guyot Stone' from the Swiss Alps to Neuchatel and
   Princeton},
Journal = {INTERNATIONAL JOURNAL OF EARTH SCIENCES},
Year = {2021},
Volume = {110},
Number = {8},
Pages = {3015-3022},
Month = {2021 NOV},
Abstract = {In 1890, former students of Arnold Guyot at the University of Neuchatel
   dedicated a `large glacial boulder from Neuchatel' to his memory, now in
   front of Guyot Hall at Princeton University. We established that this
   block originated with all certainty from Upper Carboniferous
   conglomerates of the Salvan-Dorenaz Basin, the relics of which crop out
   in the Alps of western Switzerland and eastern France. During the Last
   Glacial Maximum, the boulder travelled from the lower Rhone Valley on
   the back of the Pleistocene Rhone Glacier to Neuchatel from where,
   later, it crossed the Atlantic and was transported to Princeton. Guyot's
   name is familiar to geologists because of the term `guyot', introduced
   1946 by Harry Hess for `curious flat-topped peaks scattered over
   millions of square miles in the Pacific basin', many of which are now
   known to represent drowned carbonate platforms.},

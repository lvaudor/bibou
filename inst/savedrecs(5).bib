@article{ WOS:000920991800001,
Author = {Cotte, Gabriel and Soulignac, Frederic and Correia, Fabio dos Santos and
   Fallet, Matthieu and Ibelings, Bastiaan Willem and Barry, David Andrew
   and Vennemann, Torsten W.},
Title = {Controlling factors of phytoplankton distribution in the river-lake
   transition zone of a large lake},
Journal = {AQUATIC SCIENCES},

@article{ WOS:000750720700008,
Author = {Vaheddoost, Babak and Vazifehkhah, Saeed and Safari, Mir Jafar Sadegh},
Title = {A stochastic approach for the assessment of suspended sediment
   concentration at the Upper Rhone River basin, Switzerland},

@inproceedings{ WOS:000865798000048,
Author = {Benefice, Guillaume and Duron, Luc and Villanueva, Amaya and Yang, Rui},
Editor = {Gourbesville, P and Caignaert, G},
Title = {Calibration of 1D and 2D Fluvial Models with a Metamodel Based
   Optimization},
Booktitle = {ADVANCES IN HYDROINFORMATICS: MODELS FOR COMPLEX AND GLOBAL WATER
   ISSUES-PRACTICES AND EXPECTATIONS},
Series = {Springer Water},
Year = {2022},
Pages = {773-794},
Note = {SimHydro Conference - Models for Complex and Global Water Issues -
   Practices and Expectations (SymHydro), Polytech Nice Sophia, Sophia
   Antipolis, FRANCE, JUN 16-18, 2021},
Organization = {French Hydrotechn Soc; Assoc Francaise Mecanique; Univ Cote Azur,
   Polytech Nice Sophia; CNR; IRSN; EDF; DHI France; SETEC HYDRATEC},
Abstract = {In fluid dynamics, calibrating a hydraulic model is an essential step.
   It typically consists in adjusting friction coefficients defined by zone
   to better fit the observations. This step is often time-consuming, as
   the minimization of the error between hydraulic model and observations
   requires multiple iterations. These iterations are usually performed
   directly by the modeler or with a semi-automatic calibration method
   involving a metaheuristic method (Rothe et al. in Meta-heuristic
   optimization method for the calibration of friction coefficients in 1-D
   open surface channel modeling. Adv Hydroinf, 2018). This paper studies
   the use of an intermediate metamodel in order to realize the
   calibration. First, the metamodel is built from a design of experiments
   and with a supervised learning method conceived to solve regression
   problems. In this paper, three methods (gaussian process regression,
   linear regression and XGBoost) are selected to build several candidates
   for the intermediate metamodels. Metamodels validation is then performed
   through crossvalidation. Finally, the metamodels are directly assessed
   on friction coefficients estimation. Optimizations are repeated several
   times because of the equifinality problem. The distribution of the
   optimal datasets of friction coefficients over the iterations is studied
   and the best dataset of friction coefficients is applied to the
   hydraulic model. The study case is located on the river reach of
   Bourg-les-Valence (Rhone, France) and includes a 1D model (Crue10) and a
   Telemac-2D model. A complete benchmark has been performed to determine
   the best Design of Experiment and metamodel configurations. The
   developed approach appears to be relevant as it leads to satisfactory
   results while being computationally more efficient than semi-automatic
   method. The linear regression and the gaussian process regression yield
   very similar results for both the validation and the optimization step.
   XGBoost usually performs less accurately. Since the linear regression
   model is computationally less intense, it appeared to be the most
   relevant metamodeling method for this study case.},

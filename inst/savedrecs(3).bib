@inproceedings{ WOS:000347871500170,
Author = {Le Coz, J. and Pobanz, K. and Faure, J. -B. and Pierrefeu, G. and
   Blanquart, B. and Choquette, Y.},
Editor = {Munoz, RM},
Title = {Stage-discharge hysteresis evidenced by multi-ADCP measurements},
Booktitle = {RIVER FLOW 2012, VOLS 1 AND 2},
Year = {2012},
Pages = {1277-1283},
Note = {6th International Conference on Fluvial Hydraulics (River Flow), San
   Jose, COSTA RICA, SEP 05-07, 2012},
Organization = {Colegio Ingenieros Civiles Costa Rica; Int Assoc Hydro Environm Engn \&
   Res, Comm Fluvial Hydraul},
Abstract = {Stage-discharge hysteresis due to transient flow is often difficult to
   highlight during floods in natural rivers. Multi-ADCP discharge
   measurements conducted during a national ADCP intercomparison test in
   2010 on the Rhone river downstream of Genissiat dam, France, were used
   to evidence such effect. Indeed, averaging the discharges simultaneously
   measured by 14 ADCP allowed to reduce the measurement uncertainty, while
   the uncertainty due to the time-integration of a varying discharge
   remained low, due to the short duration of an ADCP transect. Applying
   metrological standards for interlaboratory tests, the combined ADCP
   uncertainty was computed to be similar to 2.2\%, ignoring the bias
   related to the ADCP method itself. The application of the Jones (1916)
   formula provided an acceptable correction of the discharge deviation to
   steady conditions, which reached 3-4\% typically. A 1D hydrodynamic
   model was also applied and confirmed that the observed stage-discharge

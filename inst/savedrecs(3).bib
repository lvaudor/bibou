Title = {Sturgeon Recovery Plan in the Rhone River (France): Preliminary Results
   on Species Determination and Habitat Suitability},
Booktitle = {BIOLOGY, CONSERVATION AND SUSTAINABLE DEVELOPMENT OF STURGEONS},
Series = {Fish and Fisheries Series},

@article{ WOS:000310085600004,
Author = {Provansal, M. and Ferrand, E. and Eyrolle, F. and Raccasi, G. and
   Monaco, M. and Gurriaran, R.},
Title = {Spatial variability in sedimentation rates and artificial radionuclide
   storage in alluvial banks of the lower Rhne River},

@article{ WOS:000304576800006,
Author = {Denninger, Claude and van Maanen, Marten},
Title = {Vegetation found along a side stretch of the Saone River, near
   Saint-Germain-au-Mont-d'Or (Rhone department, France)},
Journal = {BULLETIN MENSUEL DE LA SOCIETE LINNEENNE DE LYON},
Year = {2012},
Volume = {81},
Number = {5-6},
Pages = {117-118},
Month = {MAY-JUN},
Abstract = {This approximately 370 m long, 20 m wide, apparently current-free side
   stretch of the Saone River is home to a wide variety of plants,
   especially semi-aquatic and aquatic plants. The semi-aquatic plants
   include Acorns calamus, Carex pseudocyperus, Sagittaria sagittifolia and
   Senecio paludosus. The aquatic plants include Naias marina, Trapa natans
   and Vallisneria spiralis. On the banks of the river, Althaea
   officinalis, Bidens tripartita, Euphorbia esula and Vincetoxicum

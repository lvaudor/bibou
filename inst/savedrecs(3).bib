@article{ WOS:000330779600013,
Author = {Mondy, Cedric P. and Usseglio-Polatera, Philippe},
Title = {Using fuzzy-coded traits to elucidate the non-random role of
   anthropogenic stress in the functional homogenisation of invertebrate
   assemblages},

@article{ WOS:000303546900018,
Author = {Ruedel, Heinz and Mueller, Josef and Quack, Markus and Klein, Roland},
Title = {Monitoring of hexabromocyclododecane diastereomers in fish from European
   freshwaters and estuaries},
Journal = {ENVIRONMENTAL SCIENCE AND POLLUTION RESEARCH},

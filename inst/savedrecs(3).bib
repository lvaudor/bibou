@article{ WOS:000342529900005,
Author = {Eyrolle-Boyer, F. and Renaud, Ph. and Claval, D. and Tournieux, D. and
   Le Dore, F. and Blanchet, J. -F. and Loyen, J. and Antonelli, Ch. and
   Cossonnet, C. and Cagnat, X.},
Title = {Radiological quality Assessment of Rhone River filtered waters at its
   lower course in the framework of water production for human consumption
   with historical and regional perspectives},
Journal = {RADIOPROTECTION},
Year = {2014},
Volume = {49},
Number = {3},
Pages = {183-193},
Month = {JUL-SEP},
Abstract = {The public health code, including the Ministerial Decree of 11 June
   2007, set four indicators to evaluate the radiological quality of
   drinking waters (a gross activity, beta gross activity, tritium activity
   and total indicative dose), as well as guide values and quality
   references. All of the data from the monitoring show that while the
   radioactivity of the natural origin course remained constant over time,
   the levels of radioactive contamination of the Rhone from artificial
   sources declined sharply from the early 90s, i.e. 10 to 100 times
   following radionuclides. The data also emphasise that no threshold
   activities for a global, beta global and tritium was exceeded in
   filtered water of the Rhone. The total indicative dose (TID) remained
   below 100 mu Sv/year. TB) quantification from water samplings in 2011 in
   the downstream part of the Rhone River (BRL channel), Orb river and
   Herault river are all far below the quality references. The contribution
   to the TID of artificial radionuclides detected in these water systems
   was shown to be negligible (<0.01\%).},

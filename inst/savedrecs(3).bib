@article{ WOS:000336016100003,
Author = {Scapozza, Cristian and Laigre, Laetitia},
Title = {The contribution of Electrical Resistivity Tomography (ERT) in Alpine
   dynamics geomorphology: case studies from the Swiss Alps},

@inproceedings{ WOS:000268178300131,
Author = {Mayor, P. A. and Springman, S. M. and Teysseire, P.},
Editor = {Toll, DG and Augarde, CE and Gallipoli, D and Wheeler, SJ},
Title = {In situ field experiment to apply variable high water levels to a river
   levee},
Booktitle = {UNSATURATED SOILS: ADVANCES IN GEO-ENGINEERING},
Year = {2008},
Pages = {947-952},
Note = {1st European Conference on Unsaturated Soils, Durham, ENGLAND, JUL
   02-04, 2008},
Abstract = {A section of flood protection levee along the river Rhone in the canton
   of Valais has been identified as the site for a field experiment to
   investigate response due to extensive and repeated river floods. This

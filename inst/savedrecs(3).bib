@article{ WOS:000326665600008,
Author = {Ambert, Paul},
Title = {UPPER PLEISTOCENE PERIGLACIAL LANDFORMS AND DEPOSITS OF FRENCH
   MEDITERRANEAN COAST: THE EXAMPLE OF THE ETANG DE BERRE},

@article{ WOS:000275812900004,
Author = {Feuillas, Dominique},
Title = {Improving the reliability of CNR's flood spillways},
Journal = {HOUILLE BLANCHE-REVUE INTERNATIONALE DE L EAU},
Year = {2010},
Number = {1},
Pages = {28-34},
Abstract = {Ensuring the reliability of the gates the CNR's dams is a vital factor
   in controlling the flows of the Rhone river, especially during periods
   of high flow rates when they act as flood spillways. This paper presents
   the main actions performed in recent years to ensure evermore reliable
   operation. They include gate maintenance, increasing electric power
   supplies, increasing the reliability of automated operation, remote
   transmission, etc. To a great extent the actions result from feedback
   from experience of operating the equipment during significant events and
   flooding.
   In particular the paper describes the progress achieved through the use
   of computers and optical fibre communications which now provide precise
   monitoring of the liver and the development schemes from the remote
   comtrol centre at Lyon. It also recalls that safety above all depends on
   the certainty of being able to operate the gates even in abnormal mode,
   by trained personnel acting on site at the dams.},

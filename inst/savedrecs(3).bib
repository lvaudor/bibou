@article{ WOS:000273099800012,
Author = {Federico, Stefano and Avolio, Elenio and Pasqualoni, Loredana and De
   Leo, Leonardo and Sempreviva, Anna Maria and Bellecci, Carlo},
Title = {Preliminary results of a 30-year daily rainfall data base in southern
   Italy},

@article{ WOS:000300851200024,
Author = {Anyusheva, Maria and Lamers, Marc and Nguyen La and Van Vien Nguyen and
   Streck, Thilo},
Title = {Fate of Pesticides in Combined Paddy Rice-Fish Pond Farming Systems in
   Northern Vietnam},

@article{ WOS:000315176800002,
Author = {Laigre, Laetitia and Reynard, Emmanuel and Arnaud-Fassetta, Gilles and
   Baron, Ludovic and Glenz, Damian},
Title = {Characterisation of the Rhone River palaeodynamics in Central Valais
   (Switzerland) with the electrical resistivity tomography method},
Journal = {GEOMORPHOLOGIE-RELIEF PROCESSUS ENVIRONNEMENT},
Year = {2012},
Number = {4},
Pages = {405-426},
Month = {OCT-DEC},
Abstract = {Electrical resistivity tomography (ERT) is a geophysical method
   frequently used to obtain a 2D view of sediment structures. Two sets of
   measurements were carried out along two transects crossing the Swiss
   Rhone valley between Sion and Martigny. Two different electrode spacings
   were tested (1 m and 5 m), providing a maximum investigation depth of 45
   m and an efficient vertical resolution for the definition of sediment
   architecture. Different fluvial environments were prospected (proximal
   flood plains, distal flood plains, swamps, palaeochannels). The
   comparison of ERT results with historical maps and cores allowed us to
   confirm the efficiency of ERT in Alpine floodplains, by establishing a
   precise table of reference values for the Swiss Rhone River. Several
   palaeochannels were identified at different depths: the older one are
   located between 7 in and 25 in deep; the recent ones are located between
   I m and 5 m and were active during the Little Ice Age. Furthermore, ERT
   profiles have highlighted the existence of a channel with a width of 100
   m (against 40 m for the present channel). The main fluvial activity of
   the Rhodanian hydrosystem was located in the northern part of the
   valley. Moreover, several secondary channels flowed in parallel with
   that of the Rhone and joined it near Martigny. The absence of a clear
   sediment structure in the proximal flood plain of the secondary channels
   (inactive at present) emphasises the high degree of lateral variation of
   the channel. This tendency is attributed to the effects of torrential
   tributaries.},

Author = {Beniston, Martin},
Title = {Impacts of climatic change on water and associated economic activities
   in the Swiss Alps},
Journal = {JOURNAL OF HYDROLOGY},
Year = {2012},
Volume = {412},
Pages = {291-296},
Month = {JAN},

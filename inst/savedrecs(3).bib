@article{ WOS:000275532900004,
Author = {Vallet-Coulomb, Christine and Cartapanis, Olivier and Radakovitch,
   Olivier and Sonzogni, Corinne and Pichaud, Marc},
Title = {Pan-derived isotopic composition of atmospheric vapour in a
   Mediterranean wetland (Rhone River Delta, France)},
Journal = {ISOTOPES IN ENVIRONMENTAL AND HEALTH STUDIES},
Year = {2010},
Volume = {46},
Number = {1},
Pages = {37-48},
Note = {2nd Joint European Stable Isotope User Meeting (JESIUM), Presquile de
   Giens, FRANCE, AUG 31-SEP 05, 2008},
Abstract = {A continuous record of atmospheric vapour isotopic composition (A) can
   be derived from the isotope mass balance of a water body submitted to
   natural evaporation. In this paper, we present preliminary results of
   the application of this method to a drying evaporation pan, located in a
   Mediterranean wetland, during a two-month summer period. Results seem
   consistent with few atmospheric vapour data based on the assumption of
   isotopic equilibrium with precipitation, but we observed a shift between
   pan-derived A and the composition of vapour samples collected by cold
   trapping. These results suggest that further investigations are
   necessary to evaluate the effect of diurnal variations of atmospheric
   conditions on the applicability of the pan-evaporation method, and on
   the representative of grab atmospheric samples. We also propose a
   sensitivity analysis for evaluating the impact of the different measured
   components on A calculation, and show an improvement in the method
   efficiency as the pan is drying.},

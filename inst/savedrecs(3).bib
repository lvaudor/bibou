@inproceedings{ WOS:000312421200002,
Author = {Christol, Michel},
Book-Group-Author = {PUM},
Title = {``The organization of communities in Southern Gaul (Transalpine, then
   Narbonensis) under Roman rule{''}},
Booktitle = {VIVRE EN GAULE NARBONNAISE: ISIS AU CAPITOLE},

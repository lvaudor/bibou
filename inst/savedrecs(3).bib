@incollection{ WOS:000275052200006,
Author = {Bravard, Jean-Paul},
Editor = {Garrido, A and Dinar, A},
Title = {Combined impacts of development and climate change on the Rhone River
   (Switzerland and France)},
Booktitle = {MANAGING WATER RESOURCES IN A TIME OF GLOBAL CHANGE: CONTRIBUTIONS FROM
   THE ROSENBERG INTERNATIONAL FORUM ON WATER POLICY},
Series = {Contributions from the Rosenberg International Forum on Water Policy},
Year = {2009},
Pages = {67-93},
Abstract = {Most of the total discharge of the Rh ne River at the mouth (1700
   m(3).s(-1)) originates in the Alps. Climatic models anticipate a
   decrease of total discharge, a marked decrease of summer discharge, an
   increase in winter discharge, and a decrease in ice and snow cover,
   inducing a change in the river flow regime. However, the high level of
   economic development in the entire watershed has triggered complex
   impacts on river and lake hydrosystems. These already registered changes
   foreshadow predicted changes and provide insights into the future of
   aquatic ecosystems that will suffer the impacts of warmer temperatures.
   An attempt was made to summarize the possible impacts on the future uses
   of water. Hydro and thermal power will be affected by changes in
   hydrological regime and reduction in the cooling capacity of the Rh ne
   River. Also, tourism will experience a reduction of snow cover in
   winter; agriculture, a reduction of water availability in summer; human
   health will face threats of new kinds of parasites; and last, the level
   of flood risks may increase in valley bottoms following deposition of
   sediments and increased water levels.},

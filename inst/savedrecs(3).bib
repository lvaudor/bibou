@article{ WOS:000287897900010,
Author = {Bonnet, Stephane and Jorda, Christophe and Nin, Nuria},
Title = {NEW DATA ON PALEOGEOGRAPHY AND SETTLEMENT IN AIX-EN-PROVENCE
   (BOUCHES-DU-RHONE): FIRST RESULT OF GEOARCHEOLOGICAL AND
   GEOMORPHOLOGICAL APPROACH},

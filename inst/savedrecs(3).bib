@article{ WOS:000318973400028,
Author = {De Laurentiis, Elisa and Minella, Marco and Bodrato, Marco and Maurino,
   Valter and Minero, Claudio and Vione, Davide},
Title = {Modelling the photochemical generation kinetics of
   2-methyl-4-chlorophenol, an intermediate of the herbicide MCPA
   (2-methyl-4-chlorophenoxyacetic acid) in surface waters},
Journal = {AQUATIC ECOSYSTEM HEALTH \& MANAGEMENT},
Year = {2013},
Volume = {16},
Number = {2, SI},
Pages = {216-221},
Month = {APR 1},
Abstract = {2-Methyl-4-chlorophenol (MCP) is the main transformation intermediate of
   the herbicide MCPA in surface waters and it is more toxic than its
   parent compound. MCP is produced from MCPA by both direct photolysis and
   (OH)-O-center dot reaction. The latter process has higher yield of MCP
   from MCPA: 0.5vs. 0.3 for the direct photolysis. Our model results show
   that the formation rate of MCP would be higher in waters that contain
   low organic matter and high nitrate and nitrite. Such conditions are
   favourable to MCPA direct photolysis and (OH)-O-center dot-induced
   transformation, which are both inhibited by organic matter, while
   (OH)-O-center dot formation is enhanced by nitrate and nitrite. Good
   agreement is obtained between model predictions and field data of MCPA
   transformation in the Rhone river delta (Southern France). The field
   data also suggest that MCP undergoes slightly faster transformation than
   MCPA in that environment.},

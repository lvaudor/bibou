@article{ WOS:000283979700003,
Author = {Cregut-Bonnoure, Evelyne and Boulbes, Nicolas and Daujeard, Camille and
   Fernandez, Philippe and Valensi, Patricia},
Title = {NEW DATA ON THE LARGE MAMMALS FAUNA FROM THE EEMIAN IN THE SOUTHEAST OF
   FRANCE},

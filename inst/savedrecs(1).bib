@article{ WOS:A1994PD62500002,
Author = {BEAUGELINSEILLER, K and BAUDIN, JP and BROTTET, D},
Title = {USE OF AQUATIC MOSSES FOR MONITORING ARTIFICIAL RADIONUCLIDES DOWNSTREAM
   OF THE NUCLEAR-POWER-PLANT OF BUGEY (RIVER RHONE, FRANCE)},
Journal = {JOURNAL OF ENVIRONMENTAL RADIOACTIVITY},
Year = {1994},
Volume = {24},
Number = {3},
Pages = {217-233},
Abstract = {The detection of radionuclides in water, downstream of nuclear
   installations located on river banks, is often very difficult notably
   because of their low concentrations. Thus the use of biological

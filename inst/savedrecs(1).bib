@article{ WOS:A1994QA79100004,
Author = {SVOBODOVA, A},
Title = {EVOLUTION OF ARM SYSTEMS AND THEIR FUNCTIONAL TYPOLOGY (EXAMPLE OF THE
   SLOVAK HUNGARIAN DANUBE RIVER REACH)},
Journal = {EKOLOGIA-BRATISLAVA},
Year = {1994},
Volume = {13},
Number = {4},
Pages = {369-383},
Abstract = {During the years 1990-1992 a detailed research on the Danube localities
   within the Slovak-Hungarian river section was performed. Using
   geomorphological, geodetical methods, and remote sensing, the evolution
   of five arms of the Danube was analysed. This analysis was done on
   following localities: Dobrohost-''Dunajske kriviny'', Bodiky-''Bodicka
   brana'', Kralovska luka, Klucovec-''Sporna sihot'' and Vnutorna Tejka.
   Based upon achieved results, a method of functional typology of the
   `'Lyons school'' was applied. As the regime of the Rhone is similar to
   that of the Danube, and similar laws are valid for the ecosystems
   evolution for both rivers, it is possible to apply these results with
   some modifications also for the Danube arms systems. These systems were
   classified and according to their functional typology their future
   evolution was predicted.},

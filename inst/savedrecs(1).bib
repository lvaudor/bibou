@article{ WOS:A1990EQ69600012,
Author = {HEUSSNER, S and CHERRY, RD and HEYRAUD, M},
Title = {PO-210, PB-210 IN SEDIMENT TRAP PARTICLES ON A MEDITERRANEAN
   CONTINENTAL-MARGIN},
Journal = {CONTINENTAL SHELF RESEARCH},
Year = {1990},
Volume = {10},
Number = {9-11},
Pages = {989-1004},
Month = {SEP-NOV},
Abstract = {Concentrations of the natural radionuclides Pb-210 and Pb-210 were
   measured in particulate matter collected in sediment traps, deployed in
   the Gulf of Lions during the first phase of the ECOMARGE programme
   (between July 1985 and April 1986).  The data obtained are unexceptional
   in terms of the absolute concentrations measured, but show interesting
   features in the Po-210/Pb-210 activity ratios.  These are high (about 6)
   in samples collected in summer at 50 m depth, they are above unity in
   all except one of the 50 and 100 m depth samples, and are less than
   unity in most of the deeper (300 and 600m) samples.  There is a clear
   tendency for Po-210 to covary with organic matter and biogenic silica;
   likewise, for Pb-210 to covary with carbonates and refractory silicates.
   Particulate matter fluxes and nuclide fluxes into the deep traps were
   very high in winter.  The utility of the Po-210/Pb-210 activity ratios,
   as indicators of the origin of these fluxes, is highlighted.  We
   suggest, tentatively, that the deep traps in the Gulf of Lions
   continental margin were, in winter, fed mainly by a lateral transport of
   riverine particles; these, in turn, originated largely in the Rhone
   River.},

@article{ WOS:A1997YB72700010,
Author = {Hughes, FMR and Harris, T and Richards, K and Pautou, G and ElHames, A
   and Barsoum, N and Girel, J and Peiry, JL},
Title = {Woody riparian species response to different soil moisture conditions:
   laboratory experiments on Alnus incana (L.) Moench},
Journal = {GLOBAL ECOLOGY AND BIOGEOGRAPHY LETTERS},
Year = {1997},
Volume = {6},
Number = {3-4},

@article{ WOS:A1993KL01200006,
Author = {CHAPPAZ, R and BRUN, G},
Title = {NEW DATA ON THE BIOLOGY AND ECOLOGY OF A CYPRINID FISH FROM SOUTHERN
   EUROPE - LEUCISCUS-(TELESTES)-SOUFIA (RISSO, 1826)},
Journal = {COMPTES RENDUS DE L ACADEMIE DES SCIENCES SERIE III-SCIENCES DE LA
   VIE-LIFE SCIENCES},
Year = {1993},
Volume = {316},
Number = {1},
Pages = {35-41},
Month = {JAN},
Abstract = {The biology and ecology of Leuciscus soufia, a southern European
   cyprinid, were studied in the population of this species in the river
   Durance, an Alpine tributary of the Rhone, France. The species currently
   colonizes this river from the Rhone to the Serre Poncon dam (altitude
   650 m), but it does not occur in the reservoir itself, nor in the waters
   upstream. Examination of its spatial and temporal distribution revealed
   that Leuciscus soufia occupies both of the main types of facies that
   occur in the regulated part of the Durance: the shallow rifles and
   pools. Leuciscus soufia is an omnivorous species, but consumption of
   algae is particularly high in September. It feeds selectively from among
   the available animal prey. The linear growth and fecundity of the
   species are here studied in detail for the first time.},

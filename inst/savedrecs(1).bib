@article{ WOS:A1996TY69700006,
Author = {Gaullier, V and Bellaiche, G},
Title = {Liguro-Provencal diapirism: The effects of residual topography below the
   Messinian salt decollement. Insights from analogue modelling},
Journal = {COMPTES RENDUS DE L ACADEMIE DES SCIENCES SERIE II FASCICULE A-SCIENCES
   DE LA TERRE ET DES PLANETES},
Year = {1996},
Volume = {322},
Number = {3},
Pages = {213-220},
Month = {FEB 1},
Abstract = {Mapping of the Liguro-Provencal salt structures (Northwestern
   Mediterranean Basin) shows their preferential distribution around the
   Rhone deep-sea fan. The diapirs display two main orientations: N-S and
   NW-SE. Hypotheses explaining such characteristics are discussed, i.e.
   density inversion, sedimentary loading and tectonic effects. A new
   hypothesis (gravitational gliding above residual topography) is tested
   with small-scale physical models. The results highlight the effects of
   the slope, the sedimentary loading and, especially, of the residual
   topography on this diapirism.},

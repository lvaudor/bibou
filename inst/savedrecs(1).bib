@article{ WOS:A1993MU19500005,
Author = {COPP, GH},
Title = {THE UPPER RIVER RHONE REVISITED - AN EMPIRICAL-MODEL OF MICROHABITAT USE
   BY 0+ JUVENILE FISHES},
Journal = {FOLIA ZOOLOGICA},
Year = {1993},
Volume = {42},
Number = {4},
Pages = {329-340},
Abstract = {Data previously collected during a typological investigation of biotopes
   in flood plains of the upper River Rhone were used to generate an
   empirical model of microhabitat use by 0+ juveniles during late summer.
   A total of 4573 specimens, representing 21 fish species, was collected
   using Point Abundance Sampling combined with electrofishing. Roach
   Rutilus rutilus was the most frequently encountered species, though
   gudgeon Gobio gobio and perch Perca fluviatilis were the most clumped.
   The progeny of rheophilous species co-occurred in significantly higher
   than excepted frequencies (chi square, P < 0.005), with avoidance of
   water currents by most species and some preference shown for the
   presence of macrophytes. Progeny of semi-rheophils demonstrated a
   variety of preferences, with higher-than-expected frequencies of most
   species observed in low turbidity waters. In contrast, the progeny of
   most limnophilous species were significantly associated with more turbid
   waters, perhaps characteristic of the abandoned channels they inhabited,
   with some species revealing higher-than-expected frequencies in the
   presence of water current. In general, the results corroborate the
   previously reported microhabitat use 0+ juveniles investigated at
   individual biotopes from the same flood plain.},

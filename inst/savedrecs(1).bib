@article{ WOS:000081059900008,
Author = {Forget, P and Ouillon, S and Lahet, F and Broche, P},
Title = {Inversion of reflectance spectra of nonchlorophyllous turbid coastal
   waters},
Journal = {REMOTE SENSING OF ENVIRONMENT},
Year = {1999},
Volume = {68},

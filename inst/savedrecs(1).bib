@article{ WOS:A1997XA18800009,
Author = {Watkins, MS and Doherty, S and Copp, GH},
Title = {Microhabitat use by 0+ and older fishes in a small English chalk stream},
Journal = {JOURNAL OF FISH BIOLOGY},
Year = {1997},
Volume = {50},
Number = {5},
Pages = {1010-1024},
Month = {MAY},
Abstract = {The microhabitat use of two size/age classes of fish (0+, greater than
   or equal to 1+) in the River Lee, based on measurements of 14
   environmental variables, was studied using point abundance sampling by
   electrofishing over the summer and autumn of 1995. Microhabitat use by
   all cyprinid species (barbel Barbus barbus, minnow Phoxinus phoxinus,
   chub Leuciscus cephalus and gudgeon Gobio gobio) differed between 0+ and
   older(greater than or equal to 1+) with lentic, shallow, littoral
   environments being important for 0+ fishes, whereas deeper, faster areas
   in mid-channel were important for greater than or equal to 1+ fishes.
   There was more overlap in microhabitat use by 0+ juvenile cyprinids in
   the River Lee than in larger systems such as the River Danube
   (Slovakia/Hungary) and River Great Ouse (U.K.). (C) 1997 The Fisheries
   Society of the British Isles.},
Publisher = {ACADEMIC PRESS LTD},
Address = {24-28 OVAL RD, LONDON, ENGLAND NW1 7DX},
Type = {Article},
Language = {English},
Affiliation = {UNIV HERTFORDSHIRE,DEPT ENVIRONM SCI,LANDSCAPE \& ECOL RES GRP,HATFIELD AL10 9AB,HERTS,ENGLAND.},

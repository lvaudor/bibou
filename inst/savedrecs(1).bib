@article{ WOS:A1993MC72200004,
Author = {VEUTHEY, F},
Title = {THE RESTORATION OF HAZARDOUS-WASTE SITES IN ALPINE REGIONS},
Journal = {ENGINEERING GEOLOGY},
Year = {1993},
Volume = {34},
Number = {3-4},
Pages = {145-150},
Month = {SEP},
Note = {INTERNATIONAL SYMP ON MANAGEMENT OF HAZARDOUS AND RADIOACTIVE WASTE
   DISPOSAL SITES, AT THE 29TH INTERNATIONAL GEOLOGICAL CONGRESS, KYOTO,
   JAPAN, SEP 02-03, 1992},
Abstract = {The uncontrolled management of solid and liquid waste from chemical
   works and metallurgical industries caused the pollution of numerous
   areas in the alpine regions. The high permeability coefficient allowed
   the infiltration of polluting agents of the groundwater level and their
   dispersion downstream.
   The restoration of hazardous waste sites in the upper Rhone river valley
   demonstrated that: hydrogeological evaluations are essential to
   understand the infiltration and the transport of the polluting agents;
   they are essential to assess the efficiency of a decontamination or a
   stabilisation process on the environment; modelling approaches have to
   be considered cautiously; the controlling factors and decision making
   criteria have to apply to every situation.},

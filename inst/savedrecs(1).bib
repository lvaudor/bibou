@article{ WOS:A1993MM65400007,
Author = {CELLOT, B and ROSTAN, JC},
Title = {DISSOLVED ORGANIC-CARBON DYNAMICS IN THE UPPER RHONE - THE INFLUENCE OF
   SIDE-ARMS},
Journal = {REGULATED RIVERS-RESEARCH \& MANAGEMENT},
Year = {1993},
Volume = {8},
Number = {4},
Pages = {391-397},
Month = {DEC},
Abstract = {Dissolved organic carbon (DOC) dynamics were investigated over a two
   year period in the Upper Rhone River to examine the role of side-arms in
   providing DOC to the main channel in relation to discharge fluctuations,
   especially floods. Concentrations of DOC are shown to remain low in
   space and in time (average 1.5 mgl(-1)) and to be more related to global
   hydrological events (precipitation) than to any local flushing for
   backwaters during floods. The results do not support the general
   assumption that side-arms are the providers of DOC to the main channel.},

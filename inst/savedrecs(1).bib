@article{ WOS:A1997WK98700011,
Author = {Doutriaux, E},
Title = {Developing the Rhone and controlling surrounding vegetation},
Journal = {REVUE FRANCAISE D ALLERGOLOGIE ET D IMMUNOLOGIE CLINIQUE},
Year = {1997},
Volume = {37},
Number = {1},
Pages = {83-87},
Month = {JAN-FEB},
Abstract = {The French part of the River Rhone is developed by the Compagnie
   Nationale du Rhone. Downstream from Lyons, it is now navigable to
   shipping. By maintaining natural areas and building side canals, a great
   variety of environments - natural and artificial - have been created. To
   facilitate run off and to maintain the security of the development, the
   vegetation must be controlled The Rhone valley is also subject to the
   proliferation of a wide variety of exotic plant species, some of which
   are harmful to the ecology of the area or to the health of the
   inhabitants. One of these is ragweed, which is found by the Rhone on
   both natural and artificial banks. After a survey of the principles
   guiding the development of the Rhone, this article examine how
   vegetation is kept under control, how this affects on the ragweed life
   cycle, and how this weed can be kept under control.},
Publisher = {EXPANSION SCI FRANCAISE},

@article{ WOS:A1992JW02300009,
Author = {PENAZ, M and ROUX, AL and JURAJDA, P and OLIVIER, JM},
Title = {DRIFT OF LARVAL AND JUVENILE FISHES IN A BYPASSED FLOODPLAIN OF THE
   UPPER RIVER RHONE, FRANCE},
Journal = {FOLIA ZOOLOGICA},
Year = {1992},
Volume = {41},
Number = {3},
Pages = {281-288},
Abstract = {The drift of larval and juvenile fish was studied in August 1991 at two
   sites located on a former main channel of the Rhone River, now by-passed
   by the Bregnier-Cordon hydroelectric scheme. Twelve species of fish were
   recorded, with roach Rutilus rutilus, chub Leuciscus cephalus and barbel
   Barbus barbus being the most abundant (37.4, 35.2 and 13.2 \%,
   respectively). The number of drifting fish varied between samples during
   the 24-h cycle, with between 10.9 and 66.8 specimens collected per 1,000
   m-3 of water filtered. A diel pattern of drift was apparent from a
   distanct decrease of numbers during late afternoon and a sudden increase
   just after sunset. Chub, rudd Scardinius erythrophthalmus and loach
   Noemacheilus barbatulus were predominant amongst nocturnal drift,
   whereas dace Leuciscus leuciscus and minnow Phoxinus phoxinus were
   rather crepuscular.},

@article{ WOS:A1995RB62500034,
Author = {CHANGEUX, T and PONT, D},
Title = {ICHTHYOGEOGRAPHIC REGIONS AND WATERSHED SIZE IN THE FRENCH RIVER RHONE
   NETWORK},
Journal = {HYDROBIOLOGIA},
Year = {1995},

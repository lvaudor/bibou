@article{ WOS:A1996TT86800004,
Author = {LaurentiRibaud, A and Masse, JP},
Title = {Geometry and dynamics of a bioclastic and coral complex: The Barremian
   limestones of the Saint-Chamas region (Bouches-du-Rhone, France)},
Journal = {COMPTES RENDUS DE L ACADEMIE DES SCIENCES SERIE II FASCICULE A-SCIENCES
   DE LA TERRE ET DES PLANETES},
Year = {1996},
Volume = {322},
Number = {1},
Pages = {33-38},
Month = {JAN 4},
Abstract = {Within the progradational context of the Provence Urgonian platform,
   toward the Vocontian Basin, the Barremian limestones of the Saint-Chamas
   region show the lateral transition between bioclastic, coral and rudist
   facies, i.e. the transition between the inner and the outer platform
   domains. The bioclastic facies are related to spillover lobes forming
   the basement, proximally, for coral bodies which onlap the inner side of
   the lobe flank and tend to fill up the antecedent depressions. Northward
   prograding wedges are subsequently generated, with high energy gravel
   size sediments while rudist facies settle in the inner domain. The
   geometry of the gravelly bodies is relevant for carbonate hydrocarbon
   reservoir modelling. The dynamics of the sedimentary system is
   controlled by the interplay of hydrodynamic factors and biogenic supply.},

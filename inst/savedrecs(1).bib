@article{ WOS:A1993LV01200006,
Author = {COPP, GH},
Title = {MICROHABITAT USE OF FISH LARVAE AND 0+ JUVENILES IN A SMALL ABANDONED
   CHANNEL OF THE UPPER RIVER RHONE, FRANCE},
Journal = {FOLIA ZOOLOGICA},
Year = {1993},
Volume = {42},
Number = {2},
Pages = {153-164},
Abstract = {Microhabitat use by 0+ fishes in a small oxbow lake of the upper River
   Rhone was studied using Point Abundance Sampling by electrofishing. At
   each point, 11 environmental variables were evaluated; larval and
   juvenile fish were considered as separate ecospecies. Canonical
   Correspondence Analysis and chi-square analysis revealed that all older
   larvae avoided open water, but as 0+ juveniles the cyprinids (Rutilus
   rutilus, Abramis brama, Blicca bjoerkna and Scardinius erythrophthalmus)
   except Tinca tinca co-exploited the rare open areas of the otherwise
   densely vegetated backwater. Juvenile T. tinca and Lepomis gibbosus were
   significantly associated with each other (P less-than-or-equal-to
   0.001), but the former was found mostly in filamentous algae and the
   latter avoided pen areas, preferring Nuphar sp. beds. (P
   less-than-or-equal-to 0.01). Microhabitat use of 0+ cyprinids contrasted
   that in a nearby lotic channel (C o p p 1992, Env. Biol. Fish. 33:
   181-193), where 0+ juveniles strongly preferred shallow banks, In both
   channels, microhabitat use was influenced by environmental conditions.},

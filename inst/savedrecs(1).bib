@article{ WOS:000080299800015,
Author = {Mezquita, F and Griffiths, HI and Sanz, S and Soria, JM and Piñón, A},
Title = {Ecology and distribution of ostracods associated with flowing waters in
   the eastern Iberian Peninsula},
Journal = {JOURNAL OF CRUSTACEAN BIOLOGY},

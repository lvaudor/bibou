@article{ WOS:A1996VR40200014,
Author = {Guinand, B and Bouvet, Y and Brohon, B},
Title = {Spatial aspects of genetic differentiation of the European chub in the
   Rhone River Basin},
Journal = {JOURNAL OF FISH BIOLOGY},

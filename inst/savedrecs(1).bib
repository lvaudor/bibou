@article{ WOS:000071011400003,
Author = {Gollmann, G and Bouvet, Y and Karakousis, Y and Triantaphyllidis, C},
Title = {Genetic variability in Chondrostoma from Austrian, French and Greek
   rivers (Teleostei, Cyprinidae)},
Journal = {JOURNAL OF ZOOLOGICAL SYSTEMATICS AND EVOLUTIONARY RESEARCH},
Year = {1997},
Volume = {35},
Number = {4},
Pages = {165-169},
Month = {DEC},
Abstract = {Allozyme variation in Chondrostoma from Austrian, French and northern
   Creek rivers was investigated. Populations of C. nasus from the upper
   Danube catchment are genetically very similar and apparently connected
   by high levels of gene flow. Chondrostoma nasus has conserved all major
   allozyme polymorphisms during its recent range expansion into the Rhone
   basin. Chondrostoma vardarense from the Aries and Aliakmonas rivers
   share most of their allozymes with C. nasus from the upper Danube
   catchment, but have less genetic variation, a possible consequence of a
   genetic bottleneck following river capture.},

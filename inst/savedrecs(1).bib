@article{ WOS:000071745500007,
Author = {Collina-Girard, J},
Title = {Submarine sketched profiles analysis along Provence coast, using
   scuba-diving, eustatics and neotectonics results},
Journal = {COMPTES RENDUS DE L ACADEMIE DES SCIENCES SERIE II FASCICULE A-SCIENCES
   DE LA TERRE ET DES PLANETES},
Year = {1997},
Volume = {325},
Number = {12},
Pages = {955-959},
Month = {DEC},
Abstract = {In preceding papers, an original way of sketching submarine profiles was
   used to prove (Collina-Girard, 1994 and 1996) the presence of erosional
   standstill levels (-55 m, -35 m, -25 m, -17/18 m, -11 m), about 100 km
   between Marseilles (South of France, Bouches-du-Rhone) and the island of
   Port Cues (South of France, just to the west of St Tropez, Var). This
   neu, work concerns the Eastern part of Provence and particularly, the
   areas located, first to the west of the Var River, and then the area
   around Nice, east of this same river The statistical analysis, and
   comparisons of many profiles located in both areas studied proved that
   sequences of eustatic submarine cliffs are clearly uplifted (1 m higher)
   between Port Cros and the western Var area, and more clearly between
   Western Var and Nice (between 1 and 2 m). The study proves, and
   measures, therefore, neotectonic phenomena in this part of the French
   Mediterranean coast (progressie uplifting from Port Cros to Nice). The
   methods appear very efficient, and, in a very simple way, constitute,
   for good divers, a new,geological tool for pointing out neotectonic
   events in rocky coastal areas.},

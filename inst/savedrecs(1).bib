@article{ WOS:A1991FK18900009,
Author = {FESSELER, M and MULLER, B and ECKERT, J},
Title = {COMPARISON OF THE GEOGRAPHIC-DISTRIBUTION AND PREVALENCE OF ECHINOCOCCUS
   MULTILOCULARIS AND RABIES INFECTION},
Journal = {TIERARZTLICHE UMSCHAU},
Year = {1991},
Volume = {46},
Number = {5},
Pages = {287-292},
Month = {MAY},
Abstract = {A comparison of the geographic distribution and the prevalence of E.
   multilocularis and rabies infection in the endemically infected areas in
   Middle Europe showed a hig degree of correlation, it was therefore
   assumed that the distribution of E. multilocularis is similarly
   dependent on the density of the fox population as for rabies.  The
   absence of E. multilocularis in areas with a low prevalence of rabies
   indicates that E.  multilocularis requires a more dense fox population
   to become endemic.  Extensive studies of the prevalence of E. 
   multilocularis in the fox, as the final host, have not been completed
   but appear promising, particularly in areas of high rabies prevalence. 
   A reduction in the fox population is considered to be an effective
   preventive measure for E. multilocularis.  The rabies free, but E.
   multilocularis endemically infected area of Auvergne in southern France
   is one exception where these two diseases do not occur together.  The
   rabies epidemic, which has not spread beyond the river Rhone, has not
   yet reached this area.},

@article{ WOS:A1996WJ83800035,
Author = {Abernethy, VJ and Sabbatini, MR and Murphy, KJ},
Title = {Response of Elodea canadensis Michx and Myriophyllum spicatum L to
   shade, cutting and competition in experimental culture},

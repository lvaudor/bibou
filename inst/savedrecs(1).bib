@article{ WOS:A1994PD06700004,
Author = {ROGULJ, B and MARMONIER, P and LATTINGER, R and DANIELOPOL, D},
Title = {FINE-SCALE DISTRIBUTION OF HYPOGEAN OSTRACODA IN THE INTERSTITIAL
   HABITATS OF THE RIVERS SAVA AND RHONE},
Journal = {HYDROBIOLOGIA},
Year = {1994},
Volume = {287},
Number = {1},
Pages = {19-28},
Month = {JUL 15},
Note = {Symposium on Biogeography of Subterranean Crustaceans: The Effects of
   Different Scales, at the Summer Meeting of the Crustacean-Society,
   CHARLESTON, SC, JUN, 1992},
Organization = {CRUSTACEAN SOC},
Abstract = {The small-scale distributions of three hypogean ostracods were studied
   in gravel bars in the Rivers Rhone and Sava. Fabaeformiscandona wegelini
   is widely distributed, whilst Mixtacandona n. sp. and Pseudocandona
   zschokkei have more localised distributions. In the Sava River
   (Croatia), the fauna was sampled at depths of between 0.5 and 7 m, and
   from 0 to 135 m away from the main channel. The River Rhone (France),
   was sampled a depth of 0.5 m at eight stations along a 1 160 m long
   gravel bar. In both rivers, F. wegelini was most abundant in
   interstitial habitats fed by superficial waters: close to the channel in
   the River Sava and in the downwelling area in the River Rhone. The two
   other species were found in areas fed by groundwaters: in deep sediments
   and far from the channel in the River Sava and in the upwelling area in
   the River Rhone. The microdistribution of these hypogean ostracod
   species is discussed in the context of their larger-scale
   biogeographical distributions.},

@article{ WOS:A1992HY25000004,
Author = {MARTY, B and ONIONS, RK and OXBURGH, ER and MARTEL, D and LOMBARDI, S},
Title = {HELIUM-ISOTOPES IN ALPINE REGIONS},
Journal = {TECTONOPHYSICS},
